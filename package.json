{
  "name": "docker-env",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p build:libs:watch start:apps",
    "build": "run-p build:libs build:home build:projects build:blog",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rimraf ./dist",
    "start:home": "ng serve --project home --port 4200",
    "start:projects": "ng serve --project projects --port 4201",
    "start:blog": "ng serve --project blog --port 4202",
    "start:apps": "wait-on dist/common/header/public-api.d.ts && run-p start:home start:projects start:blog",
    "build:home": "ng build --project home -c production",
    "build:projects": "ng build --project projects -c production",
    "build:blog": "ng build --project blog -c production",
    "build:lib:header": "ng build @common/header",
    "build:lib:header:watch": "ng build @common/header --watch",
    "build:libs": "run-p build:lib:header",
    "build:libs:watch": "npm run clean && run-p build:lib:header:watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^16.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "typescript": "~5.1.3",
    "wait-on": "^7.0.1"
  }
}
